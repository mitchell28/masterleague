{
	"name": "svelte5",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode development",
		"build": "vite build --mode production",
		"build:dev": "vite build --mode development",
		"preview": "vite preview",
		"preview:dev": "vite preview --mode development",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:setup": "vite-node src/lib/scripts/db-setup.ts",
		"db:run-migrations": "vite-node src/lib/scripts/db-migrate.ts",
		"db:fetch-data": "vite-node src/lib/scripts/fetch-external-data.ts",
		"db:init-teams": "vite-node src/lib/scripts/init-teams.ts",
		"db:seed-fixtures": "vite-node src/lib/scripts/seed-fixtures.ts",
		"db:seed-fake-data": "vite-node src/lib/scripts/seed-fake-users-predictions.ts",
		"db:check-league-table": "vite-node src/lib/scripts/check-league-table.ts",
		"db:user-prediction-report": "vite-node src/lib/scripts/user-prediction-report.ts",
		"db:process-fixture": "vite-node src/lib/scripts/process-fixture.ts",
		"db:update-league-table": "vite-node src/lib/scripts/update-league-table.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22.13.6",
		"@types/pg": "^8.11.11",
		"dotenv": "^16.4.7",
		"drizzle-kit": "^0.30.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.20.4",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@lucide/svelte": "^0.479.0",
		"@neondatabase/serverless": "^0.10.4",
		"better-auth": "^1.2.3",
		"drizzle-orm": "^0.40.0",
		"pg": "^8.11.3",
		"tailwindcss-animate": "^1.0.7",
		"vite-node": "^3.0.8",
		"zod": "^3.24.2"
	}
}
