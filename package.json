{
	"name": "svelte5",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode development",
		"build": "vite build --mode production",
		"build:dev": "vite build --mode development",
		"preview": "vite preview",
		"preview:dev": "vite preview --mode development",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest --watch",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:run-migrations": "vite-node src/lib/scripts/db-migrate.ts",
		"db:init-teams": "vite-node src/lib/scripts/init-teams.ts",
		"db:seed-fixtures": "vite-node src/lib/scripts/seed-fixtures.ts",
		"db:setup-organization": "vite-node src/lib/scripts/setup-default-organization.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
