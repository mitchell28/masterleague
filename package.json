		"@tailwindcss/vite": "^4.1.11",
		"@trigger.dev/build": "3.3.17",
		"@types/node": "^22.16.5",
		"@types/pg": "^8.15.4",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"dotenv": "^16.6.1",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^2.46.1",
